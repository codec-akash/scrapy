        {
            "name": "scrapy (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "scrapy phone",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug"
        }
    ]
