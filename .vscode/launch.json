    "version": "0.2.0",
    "configurations": [
        {
            "name": "scrapy",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "8080"
            ],
        },
        {
            "name": "scrapy (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
